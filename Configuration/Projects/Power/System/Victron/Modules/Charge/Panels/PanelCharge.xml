<?xml version="1.0" encoding="utf-8"?>
<PanelClass Name="PanelCharge">
    <Description>Instrument panel for Victron charger</Description>
    <PanelObject ParentGlobalID="Victron.Charge" Background="ContainerBackground">
        <RowDefinition Size="24" />
        <RowDefinition />
        <ColumnDefinition />
        <ColumnDefinition />
        <Grid Row="1" Column="1">
            <ioLabel LocalID="LabelHeaderChargeInput">
                <Properties Text="CHARGE INPUT [VAC]" TypeLabel="Small" TypeColor="ElementNeutral" TextWrapping="NoWrap" />
            </ioLabel>
        </Grid>
        <Grid Row="1" Column="2">
            <ioLabel LocalID="LabelHeaderChargeOutput">
                <Properties Text="CHARGE OUTPUT [VDC]" TypeLabel="Small" TypeColor="ElementNeutral" TextWrapping="NoWrap" />
            </ioLabel>
        </Grid>
        <Grid Row="2" Column="1">
            <ioTextBlock LocalID="InstrVoltInput">
                <Properties Text="220.1" TypeColor="InstrumentEnhancedSecondary" FontFamily="NotoSans" FontSize="Huge" FontWeight="SemiBold" TextWrapping="NoWrap" Decimals="1" TextAlignment="End" />
            </ioTextBlock>
        </Grid>
        <Grid Row="2" Column="2">
            <ioTextBlock LocalID="InstVoltOutput">
                <Properties Text="14.3" TypeColor="InstrumentEnhancedSecondary" FontFamily="NotoSans" FontSize="Huge" FontWeight="SemiBold" TextWrapping="NoWrap" Decimals="1" TextAlignment="End" />
            </ioTextBlock>
        </Grid>
    </PanelObject>
</PanelClass>