<?xml version="1.0" encoding="utf-8"?>
<PanelClass Name="PanelCharge">
    <Description>Instrument panel for IDS charger</Description>
    <PanelObject ParentGlobalID="Ids.Convert" Background="ContainerBackground">
        <RowDefinition Size="32" />
        <RowDefinition Size="24" />
        <RowDefinition />
        <ColumnDefinition />
        <ColumnDefinition />
        <Grid Row="1" Column="1" SpanColumns="2">
            <ioComboBox LocalID="SelectCharger">
                <Properties />
            </ioComboBox>
        </Grid>
        <Grid Row="3" Column="2">
            <ioTextBlock LocalID="InstrVoltOutputDc">
                <Properties Text="14.2" TypeColor="InstrumentEnhancedSecondary" FontFamily="NotoSans" FontSize="Huge" FontWeight="SemiBold" TextWrapping="NoWrap" Decimals="1" TextAlignment="End" />
            </ioTextBlock>
        </Grid>
        <Grid Row="2" Column="1">
            <ioLabel LocalID="LabelHeaderChargeInput">
                <Properties Text="CHARGE INPUT [VAC]" TypeLabel="Small" TypeColor="ElementNeutral" TextWrapping="NoWrap" />
            </ioLabel>
        </Grid>
        <Grid Row="2" Column="2">
            <ioLabel LocalID="LabelHeaderChargeOutput">
                <Properties Text="CHARGE OUTPUT [VDC]" TypeLabel="Small" TypeColor="ElementNeutral" TextWrapping="NoWrap" />
            </ioLabel>
        </Grid>
        <Grid Row="3" Column="1">
            <ioTextBlock LocalID="InstrVoltInputAc">
                <Properties Text="220.1" TypeColor="InstrumentEnhancedSecondary" FontFamily="NotoSans" FontSize="Huge" FontWeight="SemiBold" TextWrapping="NoWrap" Decimals="1" TextAlignment="End" />
            </ioTextBlock>
        </Grid>
        <Grid Row="3" Column="2">
            <ioTextBlock LocalID="InstVoltOutputDc">
                <Properties Text="14.3" TypeColor="InstrumentEnhancedSecondary" FontFamily="NotoSans" FontSize="Huge" FontWeight="SemiBold" TextWrapping="NoWrap" Decimals="1" TextAlignment="End" />
            </ioTextBlock>
        </Grid>
    </PanelObject>
</PanelClass>