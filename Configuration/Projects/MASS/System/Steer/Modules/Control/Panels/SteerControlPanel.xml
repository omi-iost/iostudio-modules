                <ioButtonShape LocalID="ButtStandby" HorizontalAlignment="Left" Width="120">
                    <Properties Text="STANDBY" TypeColor="Raised" TypeShape="Action" ShapeID="Off" />
                </ioButtonShape>
            </ioPanelGrid>
        </Grid>
        <Grid Row="2" Column="1" IsStacked="TRUE" Orientation="Vertical">
            <ioPanelGrid LocalID="GridRowUpper" VerticalAlignment="Top" Height="40" Orientation="Horizontal">
                <ioButtonText LocalID="ButtonLeftSmall" HorizontalAlignment="Left" Width="120">
