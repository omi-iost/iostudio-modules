                </ioButtonText>
                <ioButtonText LocalID="ButtRightBig" HorizontalAlignment="Left" Width="120" Margin="120,0,0,0">
                    <Properties Text="+10" TypeColor="Normal" />
                </ioButtonText>
            </ioPanelGrid>
        </Grid>
    </PanelObject>
</PanelClass>