<?xml version="1.0" encoding="utf-8"?>
<ProgramClass Name="SteerProgOperate1">
    <Description>Program to operate the steering of MASS motion from CP1</Description>
    <DataItemsCollection>
        <DataListProgram Name="Steer::Cp::SteerCpData" NameObject="Data">
            <Item NameClass="SteerCpDataCp1" NameObject="DataCp1" />
        </DataListProgram>
        <DataListProgram Name="Steer::Ap::SteerApData" NameObject="Data">
            <Item NameClass="SteerApDataAp1" NameObject="DataAp1" />
        </DataListProgram>
    </DataItemsCollection>
    <PanelsCollection>
        <PanelInProgram Name="Steer::Ap::SteerApPanel" NameObject="Autopilot">
            <Item NameClass="SteerApPanelAp1" NameObject="AutopilotAp1" />
        </PanelInProgram>
        <PanelInProgram Name="Steer::Cp::CommandPanel" NameObject="Command">
            <Item NameClass="CommandPanelCp1" NameObject="CommandCp1" />
        </PanelInProgram>
        <PanelInProgram Name="Steer::Cp::ModePanel" NameObject="Mode">
            <Item NameClass="ModePanelCp1" NameObject="ModeCp1" />
        </PanelInProgram>
        <PanelInProgram Name="Steer::Cp::SetRefPanel" NameObject="Reference">
            <Item NameClass="SetRefPanelCp1" NameObject="ReferenceCp1" />
        </PanelInProgram>
    </PanelsCollection>
    <PortsCollection>
        <PortProgram Name="ioModbus::ioPortProgramModbus" NameObject="PortModbus">
            <Item NameClass="ioPortProgramModbus" NameObject="PortModbus" />
        </PortProgram>
        <PortProgram Name="ioSystem::Ports::System::ioPortProgramSystem" NameObject="PortSystem">
            <Item NameClass="ioPortProgramSystem" NameObject="PortSystem" />
        </PortProgram>
    </PortsCollection>
    <InternalFunctions>
        <Function Name="OnRequestData">
            <Inputs>
                <Argument Name="inputInt32" TypeNum="Float" />
            </Inputs>
            <Outputs>
                <Argument Name="outInt32" TypeNum="Int32" />
            </Outputs>
        </Function>
    </InternalFunctions>
    <Sequences>
        <Run>
            <RxTxData NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.OnRequestSteerData" />
            <Function TypeFunction="Internal">
                <Call Name="OnRequestData">
                    <Input Name="inputInt32" NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.FrontThrustMeas" TypeData="Data" />
                    <Output Name="outInt32" NameClass="SteerCpDataCp1" GlobalID="Steer.Cp1.SteerMode" TypeData="Data" />
                </Call>
            </Function>
        </Run>
        <GraphicsEvent NameClass="CommandPanelCp1" GlobalID="Steer.Cp1.ButtTakeRequest.Clicked">
            <RxTxData NameClass="SteerCpDataCp1" GlobalID="Steer.Cp1.TakeCommand" />
        </GraphicsEvent>
        <GraphicsEvent NameClass="CommandPanelCp1" GlobalID="Steer.Cp1.ButtRelease.Clicked">
            <RxTxData NameClass="SteerCpDataCp1" GlobalID="Steer.Cp1.Release" />
        </GraphicsEvent>
        <RxTxData NameClass="SteerCpDataCp1" GlobalID="Steer.Cp1.OnChangeCommand">
            <GraphicsData NameClass="SteerApPanelAp1" GlobalID="Steer.Ap1.LabelApSteer">
                <DataAssigned NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.HeadingRef" />
            </GraphicsData>
        </RxTxData>
        <RxTxData NameClass="SteerCpDataCp1" GlobalID="Steer.Cp1.OnChangeSteerMode">
            <GraphicsData NameClass="CommandPanelCp1" GlobalID="Steer.Cp1.LabelCommandStatus">
                <DataAssigned NameClass="SteerCpDataCp1" GlobalID="Steer.Cp1.SteerMode" />
            </GraphicsData>
        </RxTxData>
        <RxTxData NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.OnChangeRef">
            <GraphicsData NameClass="SteerApPanelAp1" GlobalID="Steer.Ap1.LabelApSteer">
                <DataAssigned NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.HeadingRef" />
            </GraphicsData>
        </RxTxData>
        <RxTxData NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.OnChangeCompass">
            <RxTxData NameClass="SteerCpDataCp1" GlobalID="Steer.Cp1.ModeWork" />
        </RxTxData>
        <RxTxData NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.OnChangeRudder" />
        <RxTxData NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.OnChangeThrust" />
        <RxTxData NameClass="SteerApDataAp1" GlobalID="Steer.Ap1.OnChangeEngine" />
        <GraphicsEvent NameClass="ModePanelCp1" GlobalID="Steer.Cp1.ButtStandby.Clicked" />
        <GraphicsEvent NameClass="ModePanelCp1" GlobalID="Steer.Cp1.ButtCourse.Clicked" />
        <GraphicsEvent NameClass="ModePanelCp1" GlobalID="Steer.Cp1.ButtWork.Clicked" />
    </Sequences>
</ProgramClass>