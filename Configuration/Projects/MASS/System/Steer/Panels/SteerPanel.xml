            <ioPanelGrid LocalID="GridModeCp1" VerticalAlignment="Top" Height="120" Margin="0,0,4,0" PanelGlobalID="Steer.Cp1.ModePanel" />
            <ioPanelGrid LocalID="GridSetRefCp1" VerticalAlignment="Top" Height="160" Margin="0,0,4,0" PanelGlobalID="Steer.Cp1.SetRefPanel" />
        </Grid>
        <Grid Row="1" Column="2" IsStacked="TRUE" Orientation="Vertical">
            <ioPanelGrid LocalID="GridCommandCp2" VerticalAlignment="Top" Height="160" Margin="4,0,0,0" PanelGlobalID="Steer.Cp2.CommandPanel" />
            <ioPanelGrid LocalID="GridModeCp2" VerticalAlignment="Top" Height="120" Margin="4,0,0,0" PanelGlobalID="Steer.Cp2.ModePanel" />
            <ioPanelGrid LocalID="GridSetRefCp2" VerticalAlignment="Top" Height="160" Margin="4,0,0,0" PanelGlobalID="Steer.Cp2.SetRefPanel" />
        </Grid>
        <Grid Row="1" Column="3">
            <ioPanelGrid LocalID="GridAp1Panel" PanelGlobalID="Steer.Ap1.SteerAp1Panel" />
        </Grid>
    </PanelObject>
</PanelClass>