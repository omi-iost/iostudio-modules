<?xml version="1.0" encoding="utf-8"?>
<PanelClass Name="SteerPanel">
    <Description>Panel for handling steering of MASS</Description>
    <PanelObject ParentGlobalID="Steer" Background="ContainerBackground">
        <RowDefinition />
        <ColumnDefinition />
        <ColumnDefinition />
        <ColumnDefinition Size="500" />
