<?xml version="1.0" encoding="utf-8"?>
<PanelClass Name="SteerPanel">
    <Description>Panel for handling steering of MASS</Description>
    <PanelObject ParentGlobalID="Steer" Background="ContainerBackground">
        <RowDefinition />
        <ColumnDefinition />
        <ColumnDefinition />
        <ColumnDefinition Size="500" />
        <Grid Row="1" Column="1" IsStacked="TRUE" Orientation="Vertical">
            <ioPanelGrid LocalID="GridCommandCpM" VerticalAlignment="Top" Height="160" Margin="0,0,4,0" PanelGlobalID="Steer.CpM.CmdPanelSteer" />
            <ioPanelGrid LocalID="GridModeCpM" VerticalAlignment="Top" Height="120" Margin="0,0,4,0" PanelGlobalID="Steer.CpM.ModePanel" />
            <ioPanelGrid LocalID="GridSetRefCpM" VerticalAlignment="Top" Height="160" Margin="0,0,4,0" PanelGlobalID="Steer.CpM.SetRefPanel" />
        </Grid>
        <Grid Row="1" Column="2" IsStacked="TRUE" Orientation="Vertical">
            <ioPanelGrid LocalID="GridCommandCpA" VerticalAlignment="Top" Height="160" Margin="4,0,0,0" PanelGlobalID="Steer.CpA.CmdPanelSteer" />
            <ioPanelGrid LocalID="GridModeCpA" VerticalAlignment="Top" Height="120" Margin="4,0,0,0" PanelGlobalID="Steer.CpA.ModePanel" />
            <ioPanelGrid LocalID="GridSetRefCpA" VerticalAlignment="Top" Height="160" Margin="4,0,0,0" PanelGlobalID="Steer.CpA.SetRefPanel" />
        </Grid>
        <Grid Row="1" Column="3">
            <ioPanelGrid LocalID="GridApMPanel" PanelGlobalID="Steer.ApM.SteerApPanel" />
        </Grid>
    </PanelObject>
</PanelClass>