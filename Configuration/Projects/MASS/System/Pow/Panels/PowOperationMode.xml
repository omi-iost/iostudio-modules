<?xml version="1.0" encoding="utf-8"?>
<PanelClass Name="PowOperationMode">
    <Description>Operate power panel with mode buttons</Description>
    <PanelObject ParentGlobalID="Pow" Background="ContainerBackground">
        <RowDefinition />
        <ColumnDefinition />
        <Grid Row="1" Column="1" IsStacked="TRUE" Orientation="Vertical">
            <ioPanelGrid LocalID="GridPowCommand" VerticalAlignment="Top" Height="150" PanelGlobalID="Pow.PowOperation" />
            <ioPanelGrid LocalID="GridPowModes" PanelGlobalID="Pow.PowModes" />
        </Grid>
    </PanelObject>
</PanelClass>