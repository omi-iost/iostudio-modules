<?xml version="1.0" encoding="utf-8"?>
<PanelClass Name="PowCommandModePanel">
    <Description>Power command and mode panel stacked</Description>
    <PanelObject ParentGlobalID="Pow.CpM" Background="ContainerBackground">
        <RowDefinition />
        <ColumnDefinition />
        <Grid Row="1" Column="1" IsStacked="TRUE" Orientation="Vertical">
            <ioPanelGrid LocalID="CommandNoRef" VerticalAlignment="Top" Height="150" PanelGlobalID="Pow.CpM.PowCommandPanel" />
            <ioPanelGrid LocalID="ModeNoRef" PanelGlobalID="Pow.CpM.PowModePanel" />
        </Grid>
    </PanelObject>
    <PanelObject ParentGlobalID="Pow.CpA" Background="ContainerBackground">
        <RowDefinition />
        <ColumnDefinition />
        <Grid Row="1" Column="1" IsStacked="TRUE" Orientation="Vertical">
            <ioPanelGrid LocalID="CommandNoRef" VerticalAlignment="Top" Height="150" PanelGlobalID="Pow.CpA.PowCommandPanel" />
            <ioPanelGrid LocalID="ModeNoRef" PanelGlobalID="Pow.CpA.PowModePanel" />
        </Grid>
    </PanelObject>
</PanelClass>