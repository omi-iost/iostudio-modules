            <ioShape LocalID="ShapeEthernet" Margin="8,0,0,0">
                <Properties TypeShape="Automation" ShapeID="System" TypeColor="AlertAlarm" />
            </ioShape>
            <ioShape LocalID="ShapeIO" Margin="8,0,0,0">
                <Properties TypeShape="Automation" ShapeID="IO" TypeColor="AlertAlarm" />
            </ioShape>
            <ioShape LocalID="ShapeInCommand" Margin="8,0,0,0">
                <Properties TypeShape="Command" ShapeID="NotInCommand" TypeColor="InstrumentEnhancedTertiary" />
            </ioShape>
        </Grid>
    </PanelObject>
</PanelClass>